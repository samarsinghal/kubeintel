# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-aws-account-id

# Strands Configuration
AGENT_STRANDS_MODEL=us.anthropic.claude-3-5-sonnet-20241022-v2:0
AGENT_STRANDS_REGION=us-east-1
AGENT_STRANDS_SESSION_TTL=7200
AGENT_STRANDS_DISABLE_STREAMING=true
AGENT_STRANDS_FALLBACK_MODEL=us.anthropic.claude-3-5-haiku-20241022-v1:0

# Application Configuration
AGENT_HOST=0.0.0.0
AGENT_PORT=8000
AGENT_LOG_LEVEL=info
AGENT_ANALYSIS_TIMEOUT=300

# Timeout Configurations
AGENT_PREDICTIONS_TIMEOUT=30
AGENT_FILE_OPERATIONS_TIMEOUT=10
AGENT_BATCH_COMMAND_TIMEOUT=120
AGENT_SINGLE_COMMAND_TIMEOUT=30
AGENT_STARTUP_TIMEOUT=30
AGENT_SHUTDOWN_TIMEOUT=45

# Tool Configurations
AGENT_MAX_BATCH_COMMANDS=10
AGENT_MAX_RETRIES=3
AGENT_RETRY_DELAY=1.0
AGENT_LOG_TRUNCATION_LENGTH=50

# Telemetry Configurations
AGENT_TELEMETRY_AGENT_FLOWS_LIMIT=50
AGENT_TELEMETRY_MONITOR_FLOWS_LIMIT=100
AGENT_TELEMETRY_TRACES_LIMIT=200
AGENT_TELEMETRY_DEFAULT_QUERY_LIMIT=20

# Monitor Configurations
AGENT_SESSION_DIRECTORY=/tmp/kubeintel_sessions
